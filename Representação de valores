//Representação dos valores
// a) x = 1.34 x 2.3

int main ( )
{
#include<stdio.h>

float a = 1.34, c = 2.3, x;

x = a*c;

printf ("%f", x);

return 0;
}

// b) x = 20.423 + 90.5

int main ( )
{
    
#include<stdio.h>

float a = 20.423, c = 90.5, x;

x = a+c;

printf ("%f", x);

return 0;
}

c) x = -20.5 / 3
#include <stdio.h>

int main ( )
{
    float a = -20.5 , b = 3, x;
    
    x = a/b;
    
    printf ("%f", x);
    
    return 0;
}
